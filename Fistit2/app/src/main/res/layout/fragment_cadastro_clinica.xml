<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".service.CadastroClinica">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cadastroClinica"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="190dp"
            android:layout_height="94dp"
            android:layout_marginTop="28dp"
            android:src="@drawable/img"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/msgHome"
            android:layout_width="284dp"
            android:layout_height="45dp"
            android:layout_marginStart="15dp"
            android:layout_marginTop="64dp"
            android:layout_marginEnd="30dp"
            android:text="Cadastre sua Clinica:"
            android:textSize="18sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.585"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView12" />

        <EditText
            android:id="@+id/TextNome"
            android:layout_width="246dp"
            android:layout_height="53dp"
            android:layout_marginTop="14dp"
            android:ems="10"
            android:hint="Nome Da Clinica"
            android:inputType="text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/msgHome" />

        <EditText
            android:id="@+id/TextCidade"
            android:layout_width="246dp"
            android:layout_height="53dp"
            android:layout_marginStart="82dp"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="83dp"
            android:ems="10"
            android:hint="Cidade"
            android:inputType="text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/TextNome" />

        <EditText
            android:id="@+id/textRua"
            android:layout_width="246dp"
            android:layout_height="53dp"
            android:layout_marginTop="16dp"
            android:ems="10"
            android:hint="Rua"
            android:inputType="text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/TextCidade" />

        <EditText
            android:id="@+id/TextPhone"
            android:layout_width="246dp"
            android:layout_height="53dp"
            android:layout_marginTop="15dp"
            android:ems="10"
            android:hint="Telefone"
            android:inputType="phone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textRua" />

        <Button
            android:id="@+id/buttonCriarConta"
            android:layout_width="176dp"
            android:layout_height="52dp"
            android:layout_marginStart="119dp"
            android:layout_marginTop="60dp"
            android:layout_marginEnd="116dp"
            android:text="Criar Clinica"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/TextPhone" />

        <ImageView
            android:id="@+id/agenda4"
            android:layout_width="89dp"
            android:layout_height="58dp"
            android:layout_marginStart="27dp"
            android:layout_marginTop="60dp"
            android:layout_marginEnd="30dp"
            android:src="@drawable/img_3"
            app:layout_constraintEnd_toStartOf="@+id/perfil4"
            app:layout_constraintStart_toEndOf="@+id/home4"
            app:layout_constraintTop_toBottomOf="@+id/buttonCriarConta" />

        <ImageView
            android:id="@+id/perfil4"
            android:layout_width="80dp"
            android:layout_height="57dp"
            android:layout_marginStart="17dp"
            android:layout_marginTop="171dp"
            android:layout_marginEnd="37dp"
            android:src="@drawable/img_4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/agenda4"
            app:layout_constraintTop_toBottomOf="@+id/TextPhone" />

        <ImageView
            android:id="@+id/home4"
            android:layout_width="82dp"
            android:layout_height="55dp"
            android:layout_marginStart="52dp"
            android:layout_marginTop="60dp"
            android:layout_marginEnd="27dp"
            android:src="@drawable/img_1"
            app:layout_constraintEnd_toStartOf="@+id/agenda4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/buttonCriarConta" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>