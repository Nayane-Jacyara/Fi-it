<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".service.HomeClinica">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="190dp"
            android:layout_height="94dp"
            android:layout_marginTop="16dp"
            android:src="@drawable/img"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/agenda4"
            android:layout_width="89dp"
            android:layout_height="58dp"
            android:layout_marginStart="27dp"
            android:layout_marginEnd="32dp"
            android:src="@drawable/img_3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/perfil4"
            app:layout_constraintStart_toEndOf="@+id/home4" />

        <ImageView
            android:id="@+id/perfil4"
            android:layout_width="80dp"
            android:layout_height="57dp"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="35dp"
            android:layout_marginBottom="1dp"
            android:src="@drawable/img_4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/agenda4" />

        <ImageView
            android:id="@+id/home4"
            android:layout_width="82dp"
            android:layout_height="55dp"
            android:layout_marginStart="52dp"
            android:layout_marginEnd="5dp"
            android:layout_marginBottom="1dp"
            android:src="@drawable/img_1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/agenda4"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/buttonClinica"
            android:layout_width="177dp"
            android:layout_height="47dp"
            android:layout_marginStart="128dp"
            android:layout_marginTop="158dp"
            android:layout_marginEnd="130dp"
            android:text="Criar Clinica"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView11" />

        <Button
            android:id="@+id/buttonAddMedico"
            android:layout_width="184dp"
            android:layout_height="49dp"
            android:layout_marginStart="128dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="123dp"
            android:text="Adicionar Medico"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/buttonClinica" />

        <Button
            android:id="@+id/buttonListaDeMedico"
            android:layout_width="183dp"
            android:layout_height="50dp"
            android:layout_marginStart="127dp"
            android:layout_marginTop="36dp"
            android:layout_marginEnd="130dp"
            android:text="Listra de Medico"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.421"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/buttonAddMedico" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>