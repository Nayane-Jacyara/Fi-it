<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".service.CadastroMedico">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/CadastroMedico"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <ImageView
            android:id="@+id/agenda4"
            android:layout_width="89dp"
            android:layout_height="58dp"
            android:layout_marginStart="27dp"
            android:layout_marginTop="25dp"
            android:layout_marginEnd="32dp"
            android:src="@drawable/img_3"
            app:layout_constraintEnd_toStartOf="@+id/perfil4"
            app:layout_constraintStart_toEndOf="@+id/home4"
            app:layout_constraintTop_toBottomOf="@+id/buttonCriarConta" />

        <ImageView
            android:id="@+id/perfil4"
            android:layout_width="80dp"
            android:layout_height="57dp"
            android:layout_marginStart="5dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="49dp"
            android:src="@drawable/img_4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/agenda4"
            app:layout_constraintTop_toBottomOf="@+id/buttonCriarConta" />

        <ImageView
            android:id="@+id/home4"
            android:layout_width="82dp"
            android:layout_height="55dp"
            android:layout_marginStart="52dp"
            android:layout_marginTop="27dp"
            android:layout_marginEnd="11dp"
            android:src="@drawable/img_1"
            app:layout_constraintEnd_toStartOf="@+id/agenda4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/buttonCriarConta" />

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="190dp"
            android:layout_height="94dp"
            android:layout_marginTop="16dp"
            android:src="@drawable/img"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/msgHome"
            android:layout_width="265dp"
            android:layout_height="48dp"
            android:layout_marginTop="40dp"
            android:text="Adicione Medico a sua clinica:"
            android:textSize="18sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView12" />

        <EditText
            android:id="@+id/TextNome"
            android:layout_width="246dp"
            android:layout_height="53dp"
            android:layout_marginTop="16dp"
            android:ems="10"
            android:hint="Nome Do Medico"
            android:inputType="text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.49"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/msgHome" />

        <EditText
            android:id="@+id/TextCidade"
            android:layout_width="246dp"
            android:layout_height="53dp"
            android:layout_marginStart="82dp"
            android:layout_marginTop="13dp"
            android:layout_marginEnd="83dp"
            android:ems="10"
            android:hint="Especialidade"
            android:inputType="text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/TextNome" />

        <Button
            android:id="@+id/buttonCriarConta"
            android:layout_width="176dp"
            android:layout_height="52dp"
            android:layout_marginTop="12dp"
            android:text="Adiciona Medico "
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTime" />

        <EditText
            android:id="@+id/editTextCRM"
            android:layout_width="247dp"
            android:layout_height="46dp"
            android:layout_marginStart="82dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="82dp"
            android:ems="10"
            android:hint="CRM"
            android:inputType="number"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/TextCidade" />

        <EditText
            android:id="@+id/editTextValor"
            android:layout_width="247dp"
            android:layout_height="46dp"
            android:layout_marginStart="83dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="82dp"
            android:ems="10"
            android:hint="Valor da  consulta"
            android:inputType="number"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextCRM" />

        <EditText
            android:id="@+id/editTextData"
            android:layout_width="245dp"
            android:layout_height="48dp"
            android:layout_marginStart="85dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="81dp"
            android:ems="10"
            android:hint="Data disponivel"
            android:inputType="date"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextValor" />

        <EditText
            android:id="@+id/editTextTime"
            android:layout_width="247dp"
            android:layout_height="51dp"
            android:layout_marginStart="83dp"
            android:layout_marginTop="19dp"
            android:layout_marginEnd="81dp"
            android:ems="10"
            android:hint="Horario disponivel"
            android:inputType="time"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextData" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>